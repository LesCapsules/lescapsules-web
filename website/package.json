    },
    {
      "path": "./public/component---src-templates-gallery-*.js",
      "maxSize": "28KB"
    },
    {
      "path": "./public/component---src-templates-video-*.js",
      "maxSize": "4.5KB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "120B"
    },
    {
      "path": "./public/styles*.css",
      "maxSize": "4.4KB"
    }
