    "typeface-roboto-slab": "1.1.4",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "bundlesize": "0.18.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.2",
    "prettier": "2.0.5",
    "textlint": "11.6.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "prebundlesize": "gatsby build",
    "bundlesize": "bundlesize",
    "develop": "gatsby develop",
    "preview": "gatsby develop -p $PORT -H 0.0.0.0",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "34KB"
    },
    {
      "path": "./public/commons-*.js",
      "maxSize": "5.35KB"
    },
    {
      "path": "./public/component---src-pages-drink-team-*.js",
      "maxSize": "1.1KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "990B"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "3.8KB"
    },
    {
      "path": "./public/component---src-templates-gallery-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-video-*.js",
      "maxSize": "4.5KB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "120B"
    },
    {
      "path": "./public/styles*.css",
      "maxSize": "4.4KB"
    }
  ]
}
