{
  "name": "lescapsules-web",
  "description": "Website for Les Capsules",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "6.0.0",
    "@browniebroke/react-ui-components": "1.7.1",
    "@fontsource/raleway": "4.2.1",
    "@fontsource/roboto-slab": "4.2.1",
    "@sanity/block-content-to-react": "2.0.7",
    "babel-plugin-styled-components": "1.12.0",
    "gatsby": "3.1.0",
    "gatsby-plugin-google-analytics": "3.1.0",
    "gatsby-plugin-image": "1.1.0",
    "gatsby-plugin-manifest": "3.1.0",
    "gatsby-plugin-netlify": "3.1.0",
    "gatsby-plugin-offline": "4.1.0",
    "gatsby-plugin-react-helmet": "4.1.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-robots-txt": "1.5.5",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "3.1.0",
    "gatsby-plugin-sitemap": "3.1.0",
    "gatsby-plugin-styled-components": "4.1.0",
    "gatsby-plugin-typography": "3.1.0",
    "gatsby-source-filesystem": "3.1.0",
    "gatsby-source-sanity": "7.0.0",
    "gatsby-transformer-remark": "3.1.0",
    "gatsby-transformer-sharp": "3.1.0",
    "gray-percentage": "2.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "react-typography": "0.16.19",
    "slugify": "1.4.7",
    "styled-components": "5.2.1",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "bundlewatch": "0.3.2",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "prettier": "2.2.1"
  },
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss|md)\"",
    "lint:eslint": "eslint --ext .js,.jsx .",
    "lint:prettier": "prettier --check \"**/*.+(js|jsx|json|css|scss|md)\"",
    "build": "gatsby build --log-pages",
    "prebundlewatch": "gatsby build",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "preview": "gatsby develop -p $PORT -H 0.0.0.0"
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "34KB"
    },
    {
      "path": "./public/component---src-pages-drink-team-*.js",
      "maxSize": "1.1KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "990B"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "3.8KB"
    },
    {
      "path": "./public/component---src-templates-gallery-*.js",
      "maxSize": "28KB"
    },
    {
      "path": "./public/component---src-templates-video-*.js",
      "maxSize": "4.5KB"
    },
    {
      "path": "./public/styles*.css",
      "maxSize": "4.5KB"
    }
  ]
}
